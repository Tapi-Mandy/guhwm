{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f0a1790",
                "editor.background": "#0f0a1790",
                "editor.foreground": "#c3c1c5",
                "text": "#c3c1c5",
                "text.muted": "#c3c1c570",
                "text.ignored": "#c3c1c540",
                "text.placeholder": "#c3c1c550",
                "ignored": "#c3c1c530",
                "element.hover": "#c3c1c530",
                "ghost_element.hover": "#BB554B30",
                "ghost_element.selected": "#BB554B30",
                "ghost_element.active": "#BB554B60",
                "border": "#0f0a17",
                "editor.highlighted_line.background": "#BB554B10",
                "editor.active_line.background": "#BB554B10",
                "panel.background": "#0f0a1790",
                "title_bar.background": "#0f0a1790",
                "title_bar.inactive_background": "#0f0a1790",
                "status_bar.background": "#0f0a1790",
                "drop_target.background": "#0f0a1790",
                "elevated_surface.background": "#0f0a17",
                "toolbar.background": "#0f0a1790",
                "tab_bar.background": "#0f0a1790",
                "tab.inactive_background": "#0f0a1790",
                "tab.active_background": "#BB554B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f0a17",
                "scrollbar.thumb.background": "#BB554B",
                "editor.gutter.background": "#0f0a1790",
                "terminal.background": "#0f0a1710",
                "terminal.foreground": "#c3c1c5",
                "terminal.dim_foreground": "#c3c1c5",
                "terminal.bright_foreground": "#c3c1c5",
                "terminal.ansi.black": "#0f0a17",
                "terminal.ansi.red": "#BB554B",
                "terminal.ansi.green": "#E2A055",
                "terminal.ansi.yellow": "#D3B46B",
                "terminal.ansi.blue": "#2B4C9B",
                "terminal.ansi.magenta": "#2F4E9A",
                "terminal.ansi.cyan": "#566398",
                "terminal.ansi.white": "#c3c1c5",
                "terminal.ansi.bright_black": "#62596d",
                "terminal.ansi.bright_red": "#BB554B",
                "terminal.ansi.bright_green": "#E2A055",
                "terminal.ansi.bright_yellow": "#D3B46B",
                "terminal.ansi.bright_blue": "#2B4C9B",
                "terminal.ansi.bright_magenta": "#2F4E9A",
                "terminal.ansi.bright_cyan": "#566398",
                "terminal.ansi.bright_white": "#c3c1c5",
                "modified": "#BB554B",
                "syntax": {
                    "attribute": {
                        "color": "#BB554B"
                    },
                    "boolean": {
                      "color": "#E2A055"
                    },
                    "comment": {
                      "color": "#62596d"
                    },
                    "comment.doc": {
                      "color": "#62596d"
                    },
                    "constant": {
                      "color": "#E2A055"
                    },
                    "function": {
                      "color": "#566398"
                    },
                    "keyword": {
                      "color": "#2B4C9B"
                    },
                    "number": {
                      "color": "#2F4E9A"
                    },
                    "operator": {
                      "color": "#2B4C9B"
                    },
                    "string": {
                      "color": "#BB554B"
                    },
                    "variable": {
                      "color": "#E2A055"
                    }
                },
                "players": [
                  {
                    "cursor": "#BB554B",
                    "background": "#0f0a17",
                    "selection": "#BB554B30"
                  }
                ]
            }
        }
    ]
}
